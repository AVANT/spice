//--------

.el-icon-link:before {
	content: '\e68f'
}
.el-icon-twitter:before {
	content: '\e61b'
}
.el-icon-facebook:before {
	content: '\e6d9'
}
.el-icon-instagram:before {
	content: '\e698'
}
.el-icon-cc:before {
	content: '\e702'
}
