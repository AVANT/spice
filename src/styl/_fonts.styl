html,
body,
td {
	font-variant-ligatures: common-ligatures
	-webkit-font-variant-ligatures: common-ligatures
	font-variant-ligatures: common-ligatures
	-webkit-font-feature-settings: "kern"
	font-feature-settings: "kern"
	font-kerning: normal
	-moz-font-feature-settings:"liga=1, dlig=1, frac=1" 
	-moz-font-feature-settings:"liga", "dlig", "frac"  // new syntax for FF 15+
	-ms-font-feature-settings:"liga", "dlig", "frac" 
	-o-font-feature-settings:"liga", "dlig", "frac" 
	-webkit-font-feature-settings:"liga", "dlig", "frac" 
	font-feature-settings:"liga", "dlig", "frac"
}

h3,
h5,
small {
	-moz-font-feature-settings:"liga=1, dlig=1, frac=1, smcp=1" 
	-moz-font-feature-settings:"liga", "dlig", "frac", "smcp"  // new syntax for FF 15+
	-ms-font-feature-settings:"liga", "dlig", "frac", "smcp" 
	-o-font-feature-settings:"liga", "dlig", "frac", "smcp" 
	-webkit-font-feature-settings:"liga", "dlig", "frac", "smcp" 
	font-feature-settings:"liga", "dlig", "frac", "smcp"
	font-variant: small-caps
	font-size: 110%
}


